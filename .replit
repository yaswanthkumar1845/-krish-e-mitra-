run = "cd Krish-e-Mitra/fertilizer-advisory-system/backend && python -m uvicorn main:app --host 0.0.0.0 --port 8000"
modules = ["python-3.10"]

[packager]
language = "python3"
ignoredPaths = ["node_modules"]

[packager.features]
packageSearch = true
guessImports = true

[[ports]]
localPort = 8000
externalPort = 80
